{
    "name": "Exercism Plus",
    "version": "0.4",
    "description": "Mentor enhancements for the great Exercism website!",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["https://exercism.io/*"],
          "css": ["styles.css"],
          "js": ["runtime.js"],
          "run_at": "document_start"
        }
      ],
    "icons": {
        "128": "images/exercism_plus128.png"
    }
  }